<div class="page-header">
  <%= link_to administradores_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Voltar
  <% end %>
  <%= link_to edit_administradore_path(@administradore), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Editar
  <% end %>
  <h1>Mostrar administradore</h1>
</div>

<dl class="dl-horizontal">
  <dt>Nome:</dt>
  <dd><%= @administradore.nome %></dd>

  <dt>Email:</dt>
  <dd><%= @administradore.email %></dd>

  <dt>Senha:</dt>
  <dd><%= @administradore.senha %></dd>

</dl>
